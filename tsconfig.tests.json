{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": ".tmp/tests",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "node:test": ["tests/types/node-test"],
      "node:assert/strict": ["tests/types/node-assert"],
      "../../../services/apiClient": ["tests/stubs/apiClient"],
      "../services/apiClient": ["tests/stubs/apiClient"],
      "src/services/apiClient": ["tests/stubs/apiClient"],
      "./src/services/apiClient": ["tests/stubs/apiClient"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "types": [],
    "lib": ["ES2020", "DOM"],
    "noResolve": true,
    "noEmit": false,
    "strict": false
  },
  "include": [
    "tests/**/*.ts",
    "src/features/auth/services/authApi.ts",
    "src/features/auth/services/session.ts",
    "src/features/auth/types.ts",
    "src/utils/devLogger.ts",
    "src/services/logger.ts"
  ]
}
